@model  RMS.Model.spProjectTaskStatus
@{
    Layout = null;
}
<style>
    .rr {
        background: #97507E !important;
    }
</style>
<div class="col-md-2 col-sm-2 col-xs-2"></div>
<div class="col-md-6 col-sm-6 col-xs-6">
    <div class="panel panel-warning">
        <div class="panel-heading rr" style="color: white;text-align: center">Tendering</div>
        <div class="panel-body">
            <div class="row">
                <div class="col-sm-12 col-lg-12">
                    <div class="form-group">
                        <label class="col-sm-3 col-md-3 col-lg-3">Tendering :</label>
                        @if (Model.TenderType == 10050)
                        {
                            <div class="col-md-5">
                                <a href="http://dgdp.gov.bd/dgdp/AP_TEN/new/index.php" style="" target="_blank">DGDP</a>
                            </div>
                        }
                        else if (Model.TenderType == 35)
                        {
                            <div class="col-md-5">
                                <a href="http://www.bnnssddhk.navy.mil.bd/index.php?option=com_content&view=article&id=22&Itemid=64" style="" target="_blank">NSSD Dhaka</a>
                            </div>
                        }
                        else if (Model.TenderType == 10051)
                        {
                               <div class="col-md-5">
                                   <a href="http://www.nsdctg.navy.mil.bd/" style="" target="_blank">NSD Ctg</a>
                            </div>
                        }

                        <div class="col-md-4">

                        </div>

                    </div>
                </div>
                <div class="col-sm-12 col-lg-12">
                    <div class="form-group">
                        <label class="col-sm-3 col-md-3 col-lg-3">PDF :</label>
                        @if (Model.TenderTypeFile != null)
                        {
                            <div class="col-md-5">
                                <a href="@Url.Action("Show", "Demand", new { url = Model.TenderTypeFile })" style="" target="_blank"><img src="~/Content/icons/32x32/pdf.png" /></a>
                            </div>
                        }
                        else
                        {
                            <div class="col-md-5">
                            </div>
                        }

                        <div class="col-md-4">

                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


